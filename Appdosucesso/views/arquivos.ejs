<html lang="pt-br">
  <head>
    <title>Contratos</title>
    <meta charset="utf-8">
  </head>
  <body>

      <% if(docs.length !== 0) { %>
      <ul>
        <% docs.forEach(function(item) { %>
        <li>
          <% console.log(item) %>
          <a href="/download/<%= item.idContrato %>/<%= item.idDocumento %>"><%= item.name %></a>
        </li>
        <% }) %>
      </ul>
      <% } else { %>
        <p>Nenhum contrato foi encontrado!</p>
      <% } %>
      <form action="/upload/<%= item.idContrato %>/<%= item.idDocumento %>" method="post" enctype="multipart/form-data">
        <input type="file" name="uploaded" value="upload" multiple>
        <input type="submit" value="upload">
      </form>
  </body>
</html>